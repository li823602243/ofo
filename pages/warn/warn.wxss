.title-bar{
    height: 39px;
    line-height: 39px;
    font-size: 12px;
    padding-left: 20px
}
.warn-center{
    background-color: #ffffff
}
.warn-row{
   display: flex;
   flex-direction: row;
   justify-content: space-around;
   padding: 10px;
}
.warn-col{
    width: 145px;
    height: 34px;
    line-height: 34px;
    text-align: center;
    border-radius:10px ;
    border: 1px solid gainsboro
}
.warn-photo{
    width: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap:wrap;
    align-items: center;
    background-color:#ffffff;
    padding-left:25px;
}
.warn-avater{
    margin-right: 15px;
    position: relative;
    margin-top: 10px;
   margin-bottom: 10px;
    width:70px;
    height:70px;
    border-radius:5px ;
}
.warn-avater-childer{
  position: absolute;
  top: -3px;
  right: -3px;
  width: 15px;
  height: 15px;
}
.wx-checkbox-input{
     width: 145px;
    height: 34px;
}

.warn-input{
    margin-top:15px;
    height: 100px;
    background-color: #ffffff
}
.warn-input input{
    height: 50px;
    line-height: 50px;
    margin-left: 25px;
    margin-right: 10px
}
.warn-input :first-child{
    border-bottom: 1px solid gainsboro
}
.btn-area{
    margin-top: 15px
}
.btn-area button{
    width: 80%;
    background-color: #fedb01
}